[displayDeveloper.jsp]

<%@page import="com.etm.entity.Task"%>
<%@page import="com.etm.entity.Employee"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<%
List<Employee> list=(List<Employee>)request.getAttribute("allData");
for(Employee employee : list){
%>
<h1><%=employee.getEmployeeId() %> <%=employee.getEmployeeName() %>
<%=employee.getEmail() %><%=employee.getPhoneNumber() %>
<%=employee.getSalary() %>
</h1>
<%
List<Task> taskList=employee.getTasks();
if(taskList!=null){
	for(Task task :taskList)
	{
%>
<h1><%=task.getTaskId() %><%=task.getTaskName() %> <%=task.getDuration() %></h1>
<% 
}
}
}
%>
</body>
</html>
